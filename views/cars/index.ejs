<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CARS!</title>
</head>
<body>
    <h1>Cars</h1>
    <ul>
        <% cars.forEach(function(cars) { %>
        <li>
            <%= cars.car %>
            -
            <%= cars.production ? "true" : "false" %>
        </li>
        <% }) %>
    </ul>
</body>
</html>